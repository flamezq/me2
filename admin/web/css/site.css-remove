hr.style13 {
    height: 10px;
    border: 0;
    box-shadow: 0 10px 10px -10px #8c8b8b inset;
}


hr.style14 { 
  border: 0; 
  height: 1px; 
  background-image: -webkit-linear-gradient(left, #f0f0f0, #00c0ef, #f0f0f0);
  background-image: -moz-linear-gradient(left, #f0f0f0, #00c0ef, #f0f0f0);
