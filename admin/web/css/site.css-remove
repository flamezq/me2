.modal-full .modal {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  /*overflow: hidden;*/
}
.modal-full .modal-dialog {
  position: fixed;
  margin: 0;
  padding: 0;
  height: 100%;
  width: 100%;
}
.modal-full .modal-header {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  border: none;
  height: 45px;
  padding: 10px;

 
  
  background: rgb(52,180,149); 
  /* For browsers that do not support gradients */
  /* For Safari 5.1 to 6.0 */
  background: -webkit-linear-gradient(left,rgb(52,180,149),rgb(52,175,149),rgb(52,180,149));
  /* For Opera 11.1 to 12.0 */
  background: -o-linear-gradient(left,rgb(52,180,149),rgb(52,175,149),rgb(52,180,149));
  /* For Fx 3.6 to 15 */
  background: -moz-linear-gradient(left,rgb(52,180,149),rgb(52,175,149),rgb(52,180,149));
  /* Standard syntax */
  background: linear-gradient(to left, rgb(52,180,149),rgb(52,175,149),rgb(52,180,149)); 
  /*background: rgb(0,153,102);*/

  
  /*background: rgb(0,160,102); */
  /* For browsers that do not support gradients */
  /* For Safari 5.1 to 6.0 */
  /*background: -webkit-linear-gradient(left,rgb(0,180,102),rgb(0,160,102),rgb(0,180,102));*/
  /* For Opera 11.1 to 12.0 */
  /*background: -o-linear-gradient(left,rgb(0,180,102),rgb(0,160,102),rgb(0,180,102));*/
  /* For Fx 3.6 to 15 */
  /*background: -moz-linear-gradient(left,rgb(0,180,102),rgb(0,160,102),rgb(0,180,102));*/
  /* Standard syntax */
  /*background: linear-gradient(to left, rgb(0,180,102),rgb(0,160,102),rgb(0,180,102));*/ 



  color: #FFF;
}
.modal-full .modal-content {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  border-radius: 0;
  box-shadow: none;
}
.modal-full .modal-body {
  position: absolute;
  top: 50px;
  bottom: 0;
  font-size: 15px;
  overflow: auto;
  margin-bottom: 45px;
  padding: 0 15px 0;
  width: 100%;
}
.modal-full .modal-footer {
    position: absolute;
    right: 0;
    bottom: 0;
    left: 0;
    height: 50px;
    padding: 10px;
    
    color:#FFF;

    background: rgb(52,180,149); 
    /* For browsers that do not support gradients */
    /* For Safari 5.1 to 6.0 */
    background: -webkit-linear-gradient(left,rgb(52,180,149),rgb(52,175,149),rgb(52,180,149));
    /* For Opera 11.1 to 12.0 */
    background: -o-linear-gradient(left,rgb(52,180,149),rgb(52,175,149),rgb(52,180,149));
    /* For Fx 3.6 to 15 */
    background: -moz-linear-gradient(left,rgb(52,180,149),rgb(52,175,149),rgb(52,180,149));
    /* Standard syntax */
    background: linear-gradient(to left, rgb(52,180,149),rgb(52,175,149),rgb(52,180,149)); 

    /*background: rgb(0,160,102); */
    /* For browsers that do not support gradients */
    /* For Safari 5.1 to 6.0 */
    /*background: -webkit-linear-gradient(left,rgb(0,180,102),rgb(0,160,102),rgb(0,180,102));*/
    /* For Opera 11.1 to 12.0 */
